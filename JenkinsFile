pipeline {
    agent any 
	
	environment{
		strPath = "C:\\Devaraj\\Test\\"
	}
	
	
    stages {
        stage('A') {
            steps {
			
				script{
					strFileName = "a.vbs"
				}
				
                bat("${strPath} + ${strFileName}")
            }
        }
		
		stage('B')
		{
			steps{
				script{
					strFileName = "b.vbs"
				}
				
                bat("${strPath} + ${strFileName}")
			}
		}
		
		stage('C')
		{
			steps{
				script{
					strFileName = "c.vbs"
				}
				
                bat("${strPath} + ${strFileName}")
			}
		}
		
		stage('D')
		{
			steps{
				script{
					strFileName = "d.vbs"
				}
				
                bat("${strPath} + ${strFileName}")
			}
		}
    }
}